{
  "expo": {
    "name": "ScoreKings",
    "slug": "scorekings",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/scorekings_1024x1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/scorekings_1024x1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3f6d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fantasysportsapp1.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app requires your location to verify eligibility for contests and deposits based on your current state.",
        "NSCameraUsageDescription": "ScoreKings uses the camera to allow you to securely scan your payment card for faster deposits."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/scorekings_1024x1024.png",
        "backgroundColor": "#1e3f6d"
      },
      "package": "com.fantasysportsapp1.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ScoreKings to use your location to verify state eligibility."
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.fantasysportsapp1",
          "enableGooglePay": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fde4e477-2598-4b10-af95-5b011f84603c"
      }
    },
    "owner": "quintonmills98s-organization",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fde4e477-2598-4b10-af95-5b011f84603c"
    }
  }
}
